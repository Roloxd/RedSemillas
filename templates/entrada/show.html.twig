{% extends 'base.html.twig' %}

{% block title %}Entrada{% endblock %}

{% block body %}
    <h1>Entrada</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entrada.id }}</td>
            </tr>
            <tr>
                <th>Codigo_entrada</th>
                <td>{{ entrada.codigoEntrada }}</td>
            </tr>
            <tr>
                <th>Num_pasaporte</th>
                <td>{{ entrada.numPasaporte }}</td>
            </tr>
            <tr>
                <th>Cantidad</th>
                <td>{{ entrada.cantidad }}</td>
            </tr>
            <tr>
                <th>Superficie_cultivo</th>
                <td>{{ entrada.superficieCultivo }}</td>
            </tr>
            <tr>
                <th>Fecha_entrada</th>
                <td>{{ entrada.fechaEntrada ? entrada.fechaEntrada|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Tipo_entrada</th>
                <td>{{ entrada.tipoEntrada }}</td>
            </tr>
            <tr>
                <th>Observaciones</th>
                <td>{{ entrada.observaciones }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('entrada_index') }}">back to list</a>

    <a href="{{ path('entrada_edit', {'id': entrada.id}) }}">edit</a>

    {{ include('entrada/_delete_form.html.twig') }}
{% endblock %}
