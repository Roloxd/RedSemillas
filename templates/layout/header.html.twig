{# templates/header.html.twig #}
<header id="header">

   <div id="topline" class="do-sizing-explicit">
      <div class="topline__in">

         <div id="topline-left">
            <div id="header-logo-title">
               <div id="header-logo">
                  <a href="{{ path('index') }}" class="link-see" data-ids="home">
                     <img src="{{ asset('images/img/logo_logo_rgcs_small_white.jpg') }}" alt="logo"></a>
               </div>
               <div id="header-title" class="hide">
                  <a href="" class="link-see" data-ids="home">
                     <h1>RGCS</h1>
                  </a>
               </div>
            </div>
         </div>

         <div id="topline-right">
            <div class="top_login">
               {# {% if is_granted('ROLE_USER') %}
               {{ 'hola' | trans }}
               <a href="admin">{{ app.user.userName }}</a>
               |
               <a href="{{ path('logout') }}">{{ 'logout' | trans }}</a>
               {% else %}
               {{ knp_menu_render('topMenu') }} #}
            {# {% endif %} #}
            </div>
            <div id="topsearch-outer">
